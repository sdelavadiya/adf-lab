{
	"name": "DelimitedText1",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FolderName": {
				"type": "string"
			},
			"FileName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@if(equals(dataset().FileName, null), null, concat(dataset().FileName, '_', formatDateTime(utcnow(), 'yyyyMMddHHmmss')))\n",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().FolderName",
					"type": "Expression"
				},
				"fileSystem": "sqlserver-files"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}